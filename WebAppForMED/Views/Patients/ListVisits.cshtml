@model WebAppForMED.Models.Patient


<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Записи на приём")
        </th>
        <th>
            @Html.DisplayName("Врач")
        </th>
        <th>
            @Html.DisplayName("ФИО")
        </th>
        <th></th>
    </tr>

    @foreach (var item in (SelectList)ViewBag.VisitsList)
    {
        <tr>
            <td>
                @Html.DisplayName(item.Text)
            </td>
            <td>
                @Html.DisplayName((string)ViewData[item.Text])
            </td>
            <td>
                @Html.DisplayName((string)ViewData[(string)ViewData[item.Text]])
            </td>
            <td>
                @Html.ActionLink("Удалить из списка записей", "DeleteVisit", new { patientId = Model.Id, id = item.Value })
            </td>
        </tr>
    }

</table>