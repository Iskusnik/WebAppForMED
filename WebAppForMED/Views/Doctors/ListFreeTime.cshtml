@model WebAppForMED.Models.Doctor

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Свободное время для записи")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.FreeTime)
    {
        <tr>
            <td>
                @Html.Raw(item.StartTime.ToString())
            </td>
            <td>
                @Html.ActionLink("Удалить", "DeleteFreeTime", new { doctorId = Model.Id, id = item.Id })
            </td>
        </tr>
    }

    </table>